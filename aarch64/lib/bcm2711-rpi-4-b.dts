			reg = <0x7ec00000 0x00 0x4000 0x7ec04000 0x00 0x4000>;
			reg-names = "hub\0core0";
			power-domains = <0x34 0x01>;
			resets = <0x34 0x00>;
			clocks = <0x16 0x05>;
			clocks-names = "v3d";
			interrupts = <0x00 0x4a 0x04>;
			status = "disabled";
			phandle = <0xd8>;
		};
	};

	gpu {
		compatible = "brcm,bcm2711-vc5";
		status = "disabled";
		phandle = <0xd9>;
	};

